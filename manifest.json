{
	"id": "{PLUGIN_ID}",
	"name": "AdobeXD Menu Plugin",
	"version": "0.0.1",
	"description": "AdobeXD Menu Plugin",
	"summary": "An AdobeXD plugin for creating a rectangle and apply the comic sans font on texts",
	"languages": ["en"],
	"author": "Iago Barboza",
	"helpUrl": "https://github.com/iagobarboza/adobexd-command-plugin",
	"host": {
		"app": "XD",
		"minVersion": "13.0"
	},
	"uiEntryPoints": [
		{
			"type": "menu",
			"label": "Create Retangle",
			"commandId": "createRectangle",
			"shortcut": {
				"mac": "Cmd+Alt+C",
				"windows": "Cmd+Alt+C"
			}
		},
		{
			"type": "menu",
			"label": "Apply Comic Sans",
			"commandId": "applyComicSans",
			"shortcut": {
				"mac": "Cmd+Alt+A",
				"windows": "Cmd+Alt+A"
			}
		},
		{
			"type": "menu",
			"label": "Create Rainbow Text",
			"commandId": "createRainbowText",
			"shortcut": {
				"mac": "Cmd+Alt+R",
				"windows": "Cmd+Alt+R"
			}
		}
	]
}